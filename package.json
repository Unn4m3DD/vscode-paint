{
  "name": "vscode-paint",
  "version": "0.2.0",
  "publisher": "xUnn4m3DD",
  "displayName": "vscode-paint",
  "description": "vscode sketch editor",
  "repository": {
    "url": "https://github.com/Unn4m3DD/vscode-paint.git",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:vscode-paint.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-paint.open",
        "title": "Open vscode paint"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && tsc -p ./src/",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.0.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "eslint": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}